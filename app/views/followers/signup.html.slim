h1 Create Follower Account

= form_for :follower, as: :post, url: follower_signup_path() do |f|
  = f.label :username
  = f.text_field :username
  br
  = f.label :password
  = f.password_field :password
  br
  = f.label :confirm_password
  = f.password_field :password
  br
  = f.label :email
  = f.text_field :email
  br
  = f.submit 'Signup'
